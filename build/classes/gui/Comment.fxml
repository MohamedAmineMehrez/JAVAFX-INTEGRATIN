<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="577.0" prefWidth="1039.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.CommentController">
   <children>
      <Label fx:id="labelcomment" layoutX="53.0" layoutY="36.0" prefHeight="78.0" prefWidth="582.0" text="Gestion Commentaires" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="35.0" />
         </font>
      </Label>
      <TableView fx:id="tvcomment" layoutX="446.0" layoutY="122.0" onMouseClicked="#Select" prefHeight="349.0" prefWidth="536.0">
        <columns>
          <TableColumn fx:id="colid" minWidth="0.0" prefWidth="0.0" text="id" />
          <TableColumn fx:id="colarticle" prefWidth="238.0" text="id_article" />
            <TableColumn fx:id="colcomment" prefWidth="297.0" text="Commentaires" />
        </columns>
      </TableView>
      <TextField fx:id="tfsearch" layoutX="98.0" layoutY="258.0" onKeyPressed="#searchRole" onKeyReleased="#searchRole" prefHeight="31.0" prefWidth="235.0" promptText="chercher ici...." />
      <Button fx:id="btnsupprimer" layoutX="163.0" layoutY="315.0" mnemonicParsing="false" onAction="#Delete" prefHeight="31.0" prefWidth="105.0" text="Supprime" />
      <Button fx:id="btnretour" layoutX="39.0" layoutY="514.0" mnemonicParsing="false" onAction="#retourinterface" prefHeight="31.0" prefWidth="86.0" text="Retour" />
      <Button fx:id="btntraduire" layoutX="946.0" layoutY="514.0" mnemonicParsing="false" onAction="#traduire" text="traduire" />
   </children>
</AnchorPane>
